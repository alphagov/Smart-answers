<li class="done">
  <h3 class="question">
    <span class="question-number"><%= question_counter + 1 %></span><%= question.title %>
  </h3>

  <%- if question.multiple_responses? -%>
    <div class="answer multiple">
      <ul>
        <%- question.response_labels(@presenter.accepted_responses[question_counter]).each do |label| -%>
          <li><%= label %></li>
        <%- end -%>
      </ul>
    </div>
  <%- else -%>
    <div class="answer"><%= question.response_label(@presenter.accepted_responses[question_counter]) %></div>
  <%- end -%>
  <p class="undo">
    <%= link_to "Change this answer", @presenter.change_collapsed_question_link(question_counter + 1) %>
  </p>
</li>
